// eslint 설정칸 by 쪼
{
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true,
        "source.fixAll": true,
        
    },
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "[javascript, typescript, typescriptreact, json, css]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    }
}